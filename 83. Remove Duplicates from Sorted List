# 83. Remove Duplicates from Sorted List

# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def deleteDuplicates(self, head: ListNode) -> ListNode:
        head1 = head
        dic= set()
        while head:
            dic.add(head.val)
            head = head.next
        dic = list(dic)
        dic.sort()
        head = head1
        for x in dic:
            head.val = x
            prev = head
            head = head.next
        if head:
            prev.next = None
        return head1

            
            
                
            
        
