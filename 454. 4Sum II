# 454. 4Sum II
class Solution:
    def fourSumCount(self, A: List[int], B: List[int], C: List[int], D: List[int]) -> int:
        
        dic1 = defaultdict(int)
        for i in range(len(A)):
            for j in range(len(B)):
                dic1[A[i]+B[j]] += 1
        count = 0
        for i in range(len(C)):
            for j in range(len(D)):
                if -(C[i] + D[j]) in dic1:
                    count += dic1[-(C[i] + D[j])]
        return count
            
